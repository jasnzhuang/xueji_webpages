@{
    Layout = null;
    Response.ContentType = "text/xml";
    Response.Write("<?xml version=\"1.0\" encoding=\"UTF-8\"?>");
    var db = Database.Open("startersite");
    var orderid=Session["orderid"];
    var listCartCommand="SELECT mycart.sid, mycart.orderid, mycart.pcount, mycart.price, students.sname, students.ssex, (mycart.pcount*mycart.price)as summ FROM mycart INNER JOIN students ON mycart.sid = students.sid where orderid=@0";    
}
<CATALOG>
@foreach (var row in db.Query(listCartCommand,orderid)) {
    <STUDENT>
        <SNAME>@row.sname</SNAME>
        <PCOUNT>@row.pcount</PCOUNT>
        <PRICE>@row.price</PRICE>
        <SUMM>@row.summ</SUMM>
    </STUDENT>
}
</CATALOG>